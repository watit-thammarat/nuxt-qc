{
  "name": "qc",
  "version": "1.0.0",
  "description": "TNS QC INSPECTION",
  "author": "TNS IT DEVELOPMENT",
  "private": true,
  "scripts": {
    "db:seed-dev": "cross-env NODE_ENV=development sequelize db:seed:all",
    "db:migrate-dev": "cross-env NODE_ENV=development sequelize db:migrate",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "db:seed-beta": "cross-env NODE_ENV=beta sequelize db:seed:all",
    "db:migrate-beta": "cross-env NODE_ENV=beta sequelize db:migrate",
    "build:beta": "cross-env NODE_ENV=beta nuxt build",
    "run:beta": "cross-env NODE_ENV=beta node server/index.js",
    "start:beta": "npm run db:migrate-beta && npm run build:beta && npm run run:beta",
    "db:seed-production": "cross-env NODE_ENV=production sequelize db:seed:all",
    "db:migrate-production": "cross-env NODE_ENV=production sequelize db:migrate",
    "build:production": "cross-env NODE_ENV=production nuxt build",
    "run:production": "cross-env NODE_ENV=production node server/index.js",
    "start:production": "npm run db:migrate-production && npm run build:production && npm run run:production"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.4",
    "cron": "^1.4.1",
    "cross-env": "^5.2.0",
    "dotenv-flow": "^0.1.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.3.0",
    "ldapjs": "^1.0.2",
    "lodash": "^4.17.11",
    "materialize-css": "^1.0.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.0",
    "nuxt": "^2.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "qrcode": "^1.3.0",
    "sequelize": "^4.39.0",
    "uuid": "^3.3.2",
    "vue-mq": "^0.2.1",
    "vue-qrcode-reader": "^1.2.2",
    "vuelidate": "^0.7.4",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.4",
    "sequelize-cli": "^4.1.1",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0"
  }
}
